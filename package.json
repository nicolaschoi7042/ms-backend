{
  "name": "robot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "prisma generate --schema=prisma/schema.prisma && tsc",
    "start": "concurrently \"NODE_ENV=production pm2 start dist/index.js --output /roboe/app/logs/out.log --error /roboe/app/logs/error.log --log /roboe/app/logs/combined.log \" \"npx prisma studio\"",
    "dev": "nodemon",
    "seed": "ts-node seeds/seed.ts",
    "reset:db": "npx prisma migrate reset --force",
    "seed:reset": "npx prisma migrate reset --force && yarn seed",
    "test": "dotenv -e .env.test -- jest --setupFiles dotenv/config --watchAll --verbose"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.3.3",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^8.2.0",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.6.2",
    "nodemon": "^2.0.22",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/websockets": "^10.4.1",
    "@prisma/client": "^5.22.0",
    "@types/ws": "^8.5.12",
    "async-retry": "^1.3.3",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "rxjs": "^7.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ws": "^8.18.0"
  }
}
